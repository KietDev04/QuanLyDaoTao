@model IEnumerable<QuanLyDaoTaoWeb.Models.SinhVien>

@{
    ViewData["Title"] = "Danh sách Sinh viên";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Danh sách Sinh viên</h1>

<p>
    <a asp-action="CreateSinhVien" class="btn btn-primary">Thêm mới Sinh viên</a>
</p>

<table class="table">
    <thead>
        <tr>
            <th>MaSV</th>
            <th>HoTen</th>
            <th>NgaySinh</th>
            <th>Email</th>
            <th>MaKhoa</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.MaSV</td>
                <td>@item.HoTen</td>
                <td>@item.NgaySinh</td>
                <td>@item.Email</td>
                <td>@item.MaKhoa</td>
            </tr>
        }
    </tbody>
</table>